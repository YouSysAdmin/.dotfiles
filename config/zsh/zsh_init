# Various initializing functions

# Init Rbenv
if [ -d "$HOME/.rbenv" ]; then
  eval "$(rbenv init -)"
fi

# Init nodenv
if [ -d "$HOME/.nodenv" ]; then
  eval "$(nodenv init -)"
fi

# Init rust cargo
if [ -d "$HOME/.cargo" ]; then
  . "$HOME/.cargo/env"
fi

# Init goenv
if [ -d "$HOME/.goenv" ]; then
  eval "$(goenv init -)"
fi
